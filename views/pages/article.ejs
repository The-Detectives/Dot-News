<%- include('../partials/header') %>

    <!-- <link rel="stylesheet" href="../../../css/modules/article.css"> -->

    <section class="articleContainer">
        <div>
            <br>

            <p class="p_article">
                Section: <%= articleData.name %> 
            </p>

            <br>
            <h2 class="h2_article">
                <%= articleData.title %>
            </h2>
            <br>
            <br>
            <br>
            <div class="p2_article">
                <%- articleData.content %>
            </div>
            <br>
            <p class="p3_article">
                Published date: <%= articleData.published_date %>
            </p>


        </div>
        <div class="sideDiv">
            <br>
            <% articleCategory.forEach((articleCategory)=> { %>
                <div class="gallery">
                    <a href="<%= articleCategory.url %>"><img class="img_articleCat" src="<%= articleCategory.image %>">
                        <p class="p_articleCat">
                            <%= articleCategory.title%>
                        </p>
                    </a>
                </div>
                <% }); %>
        </div>
    </section>
    <link rel="stylesheet" href="../../../css/modules/article.css">
    <%- include('../partials/footer') %>